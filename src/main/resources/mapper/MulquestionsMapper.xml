<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qfedu.examsys.dao.MulquestionsDao">



    <select id="findAllMulquestions" resultType="com.qfedu.examsys.pojo.EMulquestions">
        select * from e_mulquestions
    </select>


    <delete id="delMulquestions">
        DELETE from e_mulquestions where mqId=#{mqId}
    </delete>


</mapper>